{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "esModuleInterop": true,
        "isolatedModules": false,
        "lib": [
            "esnext"
        ],
        "moduleResolution": "bundler",
        "module": "esnext",
        "noImplicitAny": true,
        "paths": {},
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
        "types": [
            "node",
            "vite/client"
        ]
    }
}
